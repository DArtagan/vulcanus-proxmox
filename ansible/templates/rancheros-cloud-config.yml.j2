hostname: vulcanus-rancheros
rancher:
  console: debian
  environment: 
    EXTRA_CMDLINE: /init
  services_include:
    kernel-headers-system-docker: true
    nfs: true
    volume-cifs: true
    volume-nfs: true
    zfs: true
  state:
    dev: LABEL=RANCHER_STATE
    wait: true
mounts:
  - ["//192.168.0.105/media", "/media/phoenix", "cifs", "rw,uid=1100,gid=1100,file_mode=0777,dir_mode=0777,username=rancher,password=rancher"]
  - ["//192.168.0.105/filesync", "/mnt/filesync", "cifs", "rw,uid=1100,gid=1100,file_mode=0777,dir_mode=0777,username=rancher,password=rancher"]
  - ["//192.168.0.105/photos", "/mnt/photos", "cifs", "rw,uid=1100,gid=1100,file_mode=0777,dir_mode=0777,username=rancher,password=rancher"]
  - ["//192.168.0.105/borg", "/mnt/borg", "cifs", "rw,uid=1100,gid=1100,file_mode=0777,dir_mode=0777,username=rancher,password=rancher"]
ssh_authorized_keys:
  - ssh-rsa  will@TheManjaroBeast 
